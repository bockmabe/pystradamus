[jira]
# the base url of your jira site, usually just the hostname
base_url: https://your.jirasite.com

# which user to log in as 
username: yourjirausername
password: supersecretpassword

# Usually estimates are stored as custom fields inside jira that have a 5-digit integer id
# do not include the text "customfield_" or "cf[...]", just put the integer id
estimate_field_id: 10101
